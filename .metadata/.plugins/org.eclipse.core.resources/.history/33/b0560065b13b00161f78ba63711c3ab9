#include <iostream>
#include <vector>
using namespace std;

#include boost/>

int main() {

	vector<int> vec;
	vec.push_back( 10 );
	vec.push_back( 20 );

	for (int i : vec )
	{
	    cout << i;
	}
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
