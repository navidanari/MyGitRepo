#include <iostream>
#include <vector>
#include <boost/foreach.hpp>
using namespace std;

#define foreach BOOST_FOREACH

int main() {

	vector<int> vec;
	vec.push_back( 10 );
	vec.push_back( 20 );
	int a[] = {1,2,3,4};
	foreach (int i, vec)//= 0; i < vec.size(); i++)
	{
	    cout << i<<"  ";
	}
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!

	return 0;
}
