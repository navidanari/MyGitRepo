#include <iostream>
#include <vector>
#include <pthread.h>
#include <unistd.h>

using namespace std;

void * th1Func(void *)
{
	cout<<"this is 1"<<endl;
}

void * th2Func(void *)
{
	cout<<"this is 2"<<endl;
}

int main() {

	vector<int> vec;
	vec.push_back( 10 );
	vec.push_back( 20 );

	pthread_t th1, th2;

	pthread_create(&th1, NULL, th1Func, NULL);
	pthread_create(&th2, NULL, th2Func, NULL);

	pthread_join(th1, NULL);
	pthread_join(th2, NULL);

	return 0;
}

