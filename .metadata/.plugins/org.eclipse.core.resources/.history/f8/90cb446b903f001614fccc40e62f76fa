#include <iostream>
#include <vector>
#include <boost/foreach.hpp>
using namespace std;

#define foreach BOOST_FOREACH

int main() {
	int a=0;
	auto b = a;
	vector<int> vec;
	vec.push_back( 10 );
	vec.push_back( 20 );

	for (int i : vec)//= 0; i < vec.size(); i++)
	{
	    cout << i<<"  ";
	}
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!

	return 0;
}
